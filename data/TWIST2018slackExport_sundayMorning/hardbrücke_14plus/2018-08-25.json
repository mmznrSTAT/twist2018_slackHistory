        "text": "## install.packages(\"data.table\")\nlibrary(data.table)\nlibrary(dplyr)\n## loading of individual CSV files\nfiles = list.files(pattern=\"*.csv\", path = \"SBB_data\", full.names = TRUE)\nSBB =  rbindlist(lapply(files, fread))\n\n## renaming the names of the columns from German to English\nsetnames(SBB, old=c(\"BETRIEBSTAG\",\"FAHRT_BEZEICHNER\",\"BETREIBER_ID\",\"BETREIBER_ABK\",\"BETREIBER_NAME\",\"PRODUKT_ID\",\"LINIEN_ID\",\"LINIEN_TEXT\",\"UMLAUF_ID\",\"VERKEHRSMITTEL_TEXT\",\"ZUSATZFAHRT_TF\",\"FAELLT_AUS_TF\",\"BPUIC\",\"HALTESTELLEN_NAME\",\"ANKUNFTSZEIT\",\"AN_PROGNOSE\",\"AN_PROGNOSE_STATUS\",\"ABFAHRTSZEIT\",\"AB_PROGNOSE\",\"AB_PROGNOSE_STATUS\",\"DURCHFAHRT_TF\"), new=c(\"OPERATING_DAY\",\"RUNNING_IDENTIFIER\",\"OPERATOR_ID\",\"OPERATOR_ABK\",\"OPERATOR_NAME\",\"PRODUCT_ID\",\"LINES_ID\",\"LINES_TEXT\",\"RETURN_ID\",\"TRANSPORT_TYPE_TEXT\",\"ADDITIONAL_TRIP_TF\",\"FOLLOWS_FROM_TF\",\"BPUIC_STOP\",\"STOPS_NAME\",\"SCHEDULED_ARRIVAL_TIME\",\"REAL_ARRIVAL_TIME\",\"REAL_ARRIVAL_TIME_STATUS\",\"DEPARTURE_TIME\",\"DEPARTURE_FORECAST\",\"DEPARTURE_FORECAST_STATUS\",\"TURNOVER_TF\"))\n\n## removing other means of transportation besides trains (boats, ships), filtering out Hardbrücke station\nSBB &lt;- filter(SBB, PRODUCT_ID == \"Zug\", STOPS_NAME == \"Zürich Hardbrücke\")\n\n## removing unnecessary columns\n##SBB &lt;- subset(SBB, select = c(OPERATING_DAY, OPERATOR_NAME, LINES_TEXT, BPUIC_STOP, ##STOPS_NAME, SCHEDULED_ARRIVAL_TIME, REAL_ARRIVAL_TIME))",
