    {
        "type": "message",
        "user": "UCFTXSG9M",
        "text": "Das ist der Code dorthin:",
        "client_msg_id": "6198ff7f-e864-4749-8368-33494dcd4d09",
        "ts": "1535225534.000100"
    },
    {
        "type": "message",
        "user": "UCFTXSG9M",
        "text": "library(\"dplyr\")\n\nzurich &lt;- read.csv(\"116_2018-08-25-14-34-18.csv\", header = TRUE)\nstr(zurich)\nzurich$Datum &lt;- as.POSIXct(zurich$Datum, format = \"%d\/%m\/%Y %H:00\")\nzurich$Date &lt;- as.Date(zurich$Datum)\n\n# get totals per date and per month\nzurich &lt;- zurich %&gt;% group_by(Date) %&gt;%\n  mutate(Total_per_Date_Schulstrasse = sum(VZS_Schulstrasse)) %&gt;%\n  ungroup() %&gt;% as.data.frame()\nzurich$Month &lt;- as.Date(paste0(substr(zurich$Date, start = 1, stop = 7), \"-01\"), factor = \"%Y-%m-%d\")\nzurich &lt;- zurich %&gt;% group_by(Month) %&gt;%\n  mutate(Total_per_Month_Schulstrasse = sum(VZS_Schulstrasse)) %&gt;%\n  ungroup() %&gt;% as.data.frame()\n\nplot(VZS_Schulstrasse ~ Date, data = zurich, type = \"l\")\nplot(Total_per_Date_Schulstrasse ~ Date, data = zurich, type = \"l\")\nplot(Total_per_Month_Schulstrasse ~ Date, data = zurich, type = \"l\")\n\nzurich.month &lt;- unique(zurich[, c(\"Month\", \"Total_per_Month_Schulstrasse\")])\nzurich.month &lt;- arrange(zurich.month, Month)\n\nzurich.ts &lt;-ts(zurich.month$Total_per_Month_Schulstrasse,\n               start = 2, frequency = 12)\nplot(zurich.ts)\n\n# dec.zurich &lt;- decompose(zurich.ts)\ndec.zurich &lt;- stl(zurich.ts, s.window = \"periodic\")\nplot(dec.zurich)",
        "client_msg_id": "3bdafd5c-de11-4b2c-9442-0c4ef00fddd2",
        "ts": "1535225537.000100"
    },
